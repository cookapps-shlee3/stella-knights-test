{
  "executionRoleArn": "arn:aws:iam::251384577117:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/unknown-knight",
          "awslogs-region": "us-west-2",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "portMappings": [
        {
          "hostPort": 8001,
          "protocol": "tcp",
          "containerPort": 8001
        }
      ],
      "environment": [
        { "name": "PRODUCTION", "value": "${PRODUCTION}"},
        { "name": "DB_USERNAME_PROD", "value": "${DB_USERNAME_PROD}"},
        { "name": "DB_PASSWORD_PROD", "value": "${DB_PASSWORD_PROD}"},
        { "name": "DB_HOST_PROD", "value": "${DB_HOST_PROD}"},
        { "name": "DB_PORT_PROD", "value": "${DB_PORT_PROD}"},
        { "name": "DB_DATABASE_PROD", "value": "${DB_DATABASE_PROD}"},
        { "name": "REDIS_HOST_PROD", "value": "${REDIS_HOST_PROD}"},
        { "name": "REDIS_PORT_PROD", "value": "${REDIS_PORT_PROD}"},
        { "name": "MONGO_URL_PROD", "value": "${MONGO_URL_PROD}"},
        { "name": "MONGO_DATABASE_PROD", "value": "${MONGO_DATABASE_PROD}"},
        { "name": "MONGO_COLLECTION_PROD", "value": "${MONGO_COLLECTION_PROD}"},
        { "name": "RABBITMQ_PROD", "value": "${RABBITMQ_PROD}"},
        { "name": "RABBITMQ_ENABLE", "value": "${RABBITMQ_ENABLE}"},
        { "name": "LOCATION_CODE", "value": "${LOCATION_CODE}"}
      ],
      "ulimits": [
        {
          "softLimit": 65535,
          "hardLimit": 65535,
          "name": "nofile"
        }
      ],
      "cpu": 1024,
      "memory": null,
      "memoryReservation": 2048,
      "image": "251384577117.dkr.ecr.us-west-2.amazonaws.com/unknown-knight",
      "name": "unknown-knight"
    }
  ],
  "cpu": "1024",
  "memory": "2048",
  "taskRoleArn": "arn:aws:iam::251384577117:role/ecsTaskExecutionRole",
  "family": "unknown-knight-prod",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "runtimePlatform": {
    "operatingSystemFamily": "LINUX",
    "cpuArchitecture": null
  }
}